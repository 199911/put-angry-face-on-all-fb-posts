{
  "manifest_version": 2,

  "name": "Facebook Auto Angry",
  "description": "This give reaction to all posts in current facebook page",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "Autoangry.png"
  },

  "content_scripts" : [
    {
      "js" : [ "jquery-2.2.1.min.js", "put-angry-icon.js"],
      "matches": [ "http://*/*", "https://*/*"]
    }
  ]
}
