{
  "manifest_version": 2,

  "name": "Facebook Auto Angry",
  "description": "This give reaction to all posts in current facebook page",
  "version": "1.3",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },

  "content_scripts" : [
    {
      "js" : [
        "put-angry-icon.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
